{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "deploy:production": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -e production",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "dev": "next dev --turbopack",
    "pages:build": "npx @opennextjs/cloudflare build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@eight/auth": "workspace:*",
    "@hookform/resolvers": "^3.10.0",
    "@number-flow/react": "^0.5.10",
    "@tanstack/react-query": "^5.81.5",
    "@workspace/ui": "workspace:*",
    "axios": "^1.10.0",
    "better-auth": "^1.2.12",
    "lucide-react": "^0.475.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "sonner": "^1.7.4",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "next-dev": "link:@cloudflare/next-on-pages/next-dev",
    "typescript": "^5.7.3",
    "wrangler": "^4.23.0"
  }
}
